{
  "name": "boiler-plate-pern",
  "version": "1.0.0",
  "description": "boiler plate to use with pern stack ( PosgreSQL - Express - React - Node )",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"nodemon server/index.js\" \"npm run client\"",
    "db:create" : "npx sequelize-cli db:create",
    "db:migrate" : "npx sequelize-cli db:migrate",
    "db:seed" : "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sebastianiki/boiler-plate-pern.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sebastianiki/boiler-plate-pern/issues"
  },
  "homepage": "https://github.com/Sebastianiki/boiler-plate-pern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
